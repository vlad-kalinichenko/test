{
  "name": "vite-mui-ts",
  "version": "1.0.0",
  "description": "TypeScript + React + Redux + MUI + RRD + ESLint + Prettier => Boilerplate",
  "license": "MIT",
  "type": "module",
  "keywords": [
    "react",
    "boilerplate",
    "typescript",
    "starter",
    "vite",
    "redux",
    "material-ui",
    "rrd",
    "prettier"
  ],
  "scripts": {
    "dev": "vite --open",
    "start": "vite --open",
    "host": "vite --open --host",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier \"src/**/*.{js,jsx,ts,tsx,css,scss}\" --write",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:cover": "vitest run --coverage",
    "test:e2e": "pnpm preview:test 'cypress open'",
    "test:e2e:headless": "pnpm preview:test 'cypress run'",
    "test:e2e:ci": "vite build && pnpm preview:test 'cypress run --record'"
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@hookform/resolvers": "^4.1.2",
    "@mui/icons-material": "^6.1.3",
    "@mui/material": "^6.1.3",
    "@reduxjs/toolkit": "^2.2.8",
    "fastify": "^5.2.1",
    "notistack": "^3.0.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.54.2",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.27.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.12.0",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react-swc": "^3.7.1",
    "eslint": "^9.12.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.12",
    "globals": "^15.11.0",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.8.1",
    "vite": "^6.2.0"
  }
}
